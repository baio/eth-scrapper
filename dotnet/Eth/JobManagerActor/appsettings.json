{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.Extensions.Diagnostics.HealthChecks": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {

        "Name": "Seq",
        "Args": { "serverUrl": "http://seq:80" }
      }
    ],
    "Properties": {
      "Application": "scrapper-dispatcher-actor"
    }
  },
  "AllowedHosts": "*"
}
